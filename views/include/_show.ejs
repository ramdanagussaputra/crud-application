<% for (let i = 0; i < user.length; i++) {%>
<tr>
  <td><%= i + 1 %></td>
  <td><%= user[i].name %></td>
  <td><%= user[i].email %></td>
  <td><%= user[i].gender %></td>
  <td><%= user[i].status %></td>
  <td>
    <a
      href="/update-user?id=<%= user[i]._id %> "
      class="btn border-shadow update"
      ><i class="fa-solid fa-pencil text-gradient"></i
    ></a>

    <a data-id="<%= user[i]._id %>" class="btn border-shadow delete"
      ><i class="fa-solid fa-times text-gradient"></i
    ></a>
  </td>
</tr>
<% } %>
